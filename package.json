{
  "name": "gatsby-starter-tailwind",
  "description": "Gatsby starter styled with Tailwind",
  "version": "1.1.0",
  "author": "Taylor Bryant <taylor@bryant.io>",
  "dependencies": {
    "add": "^2.0.6",
    "bluebird": "^3.5.1",
    "gatsby": "^1.9.277",
    "gatsby-image": "^1.0.55",
    "gatsby-link": "^1.6.39",
    "gatsby-plugin-google-analytics": "^1.0.31",
    "gatsby-plugin-netlify": "^1.0.21",
    "gatsby-plugin-netlify-cms": "1.0.11",
    "gatsby-plugin-react-helmet": "^2.0.8",
    "gatsby-plugin-react-next": "^1.0.11",
    "gatsby-plugin-sharp": "^1.6.48",
    "gatsby-remark-images": "^1.5.67",
    "gatsby-remark-relative-images": "^0.1.2",
    "gatsby-source-filesystem": "^1.5.39",
    "gatsby-transformer-remark": "^1.7.33",
    "gatsby-transformer-sharp": "^1.6.27",
    "intersection-observer": "^0.5.0",
    "intersection-observer-polyfill": "^0.1.0",
    "lodash": "^4.17.5",
    "lodash-webpack-plugin": "^0.11.4",
    "netlify-cms": "1.7.0",
    "nuka-carousel": "^4.3.5",
    "react-helmet": "^5.2.0",
    "react-inlinesvg": "^0.8.1",
    "react-scroll-percentage": "^2.0.0",
    "smoothscroll-polyfill": "^0.4.3",
    "uuid": "^3.2.1"
  },
  "keywords": [
    "gatsby",
    "tailwind",
    "tailwindcss",
    "purgecss"
  ],
  "license": "MIT",
  "scripts": {
    "build:css": "postcss src/layouts/index.tailwind.css -o src/layouts/generated.css",
    "watch:css": "postcss src/layouts/index.css -o src/layouts/generated.css -w",
    "build": "npm run build:css && purgecss -c ./purgecss.config.js -o ./src/layouts && gatsby build",
    "develop": "npm run watch:css & gatsby develop",
    "format": "prettier --write src/**/*.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "cssnano": "^3.10.0",
    "postcss-cli": "^5.0.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-nested": "^3.0.0",
    "prettier": "^1.11.1",
    "purgecss": "^1.0.1",
    "tailwindcss": "^0.6.1",
    "tailwindcss-aspect-ratio": "^1.0.1"
  }
}
